* {font-size:10pt;font-family:Verdana;}
a {font-weight:normal;color:#0000FF;text-decoration:none;}
a:visited {color:#0000FF;text-decoration:none;}
a:hover {text-decoration:underline;}
a.current {font-weight:normal;color:#000000;text-decoration:none;}
a.inactive {font-weight:normal;color:#000000;text-decoration:none;}
img {border:0;}
body {margin:3 0 0 0;background-color:C1CFBA;}
h3 {font-size:12pt;margin:0 0 10 0;}
h4 {font-size:12pt;margin:0 0 0 0;padding:0 0 0 0;}
form {margin:0 0 0 0;padding:0 0 0 0;}
table {border-collapse:collapse;}
ul.item {margin:0 0 0 15;padding:0 0 0 0;list-style:none;}
ul.last {margin:0 0 0 15;padding:0 0 0 0;list-style:none;}
select {margin:5 0 0 15;}
#main td {padding:3 8 8 8;vertical-align:top;}
#singleImage {text-align:center;margin:0 0 0 0;padding:3 8 8 8;width:450px;}
#left {border-bottom:2px solid #000;border-top:2px solid #000;border-right:2px solid #000;padding:3 8 8 8;vertical-align:top;width:210;}
#right {border-bottom:2px solid #000;margin:0 0 0 0;padding:3 8 8 8;}
#right img {border:2px solid #5A5;}
#right li {margin:0;display:inline;}
#list {font-size:12pt;border:1px solid #5A5;border-collapse:collapse;}
#list td {border:1px solid #5A5;padding:0 10 0 10;}
#images {margin:0;}
#images div {text-align:center;margin:5 2 5 10;float:left;}
#main {margin:0 0 0 0;}
#main a {text-align:center;vertical-align:top;}
#main div {margin:8 8 8 8;float:left;}
#header .right {font-size:12pt;margin-right:0;padding-right:28px;}
#header td.right {text-align:right;margin:0 0 10 0;}
#headlinks a {font-size:12pt;}
tr.contact {margin-right:5px;margin-left:5px;}
h1 {font-size:16pt;font-weight:bold;}
h2 {font-size:14pt;font-weight:bold;}
